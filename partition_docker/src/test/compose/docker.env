# Docker Compose env file for SB Container Launch for maven inline rewrite
# substition vars are taken from profiles/config.properties
TIBCO_INSTALL_DIR=/opt/tibco
STREAMBASE_HOME=${STREAMBASE_HOME}
ClusterName=${CLUSTERNAME}
# use vagrant rsync path /vagrant
SB_APP_FILE=${SB_APP_FILE}
SB_APP_DIR=${SB_APP_DIR}
NODE_CONFIG=${NODE_CONFIG}
SUBSTITUTIONS=${SUBSTITUTIONS}
DEPLOYDIR=${STREAMBASE_HOME}/lib:${SB_APP_DIR}
SB_ADMIN_PORT=${SB_ADMIN_PORT}
BUILDTYPE=DEVELOPMENT

